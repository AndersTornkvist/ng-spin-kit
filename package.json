    "concurrently": "3.3.0",
    "lite-server": "2.2.2",
    "@types/core-js": "0.9.35",
    "@types/node": "7.0.5",
    "ts-loader": "2.0.0",
    "typescript": "2.1.6",
    "systemjs": "0.20.7",
