{
  "name": "ng-spin-kit",
  "version": "6.0.0",
  "description": "SpinKit (http://tobiasahlin.com/spinkit/) spinners for Angular",
  "repository": {
    "type": "git",
    "url": "https://github.com/WoltersKluwerPL/ng-spin-kit"
  },
  "licenses": [
    {
      "type": "MIT",
