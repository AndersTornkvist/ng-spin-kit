    "concurrently": "3.5.1",
    "core-js": "2.5.3",
    "lite-server": "2.3.0",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.5.5",
    "systemjs": "0.20.19",
    "ts-loader": "3.2.0",
    "typescript": "2.6.2",
    "zone.js": "0.8.18"
