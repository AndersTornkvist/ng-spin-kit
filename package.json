    "@types/node": "0.0.0",
    "ts-loader": "^1.2.2",
    "typescript": "^2.0.10",

    "systemjs": "0.19.41",
    "core-js": "2.4.1",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-rc.4",
